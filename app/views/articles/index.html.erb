<h1>Articles</h1>
<table>
  <thead>
    <tr>
      <th>
        Title
      </th>
      <th>
        Discription
      </th>
      <th>
        Action
      </th>
    </tr>
    <thead>
      <% @articles.each do |article| %>
        <tbody>
          <tr>
            <td><%= article.title %></td>
            <td><%= article.description %></td>
            <td><%= link_to "show", article %>
            </tr>
          <% end %>
        </tbody>
      </table>
      <br>
      <%= link_to "Create new Article", new_article_path %>
